<!DOCTYPE html>
<html>
<% include ../pages/header %>
<body>	<!-- Fixed navbar -->
<div class="navbar navbar-fixed-top navbar-inverse" style='margin-top:-2px;'>
	<div class="navbar-inner">
		<div class="container">
			<div class="nav-collapse collapse">
				<ul class="nav">
					<li class='active'><a class="brand" href="../" ><i class='icon-book'></i>RCE With Known Vulnerabilities <%= stage %></a></li>
				</ul>
				<ul class="nav pull-right">
					<li class='active' style='margin-top:2px;'><a href="#" ><i class="icon-user"></i>&nbsp; Login &nbsp; </a></li>
				</ul>
			</div>
		</div>
	</div>
</div>
<!-- end of fixed Nav bar-->

	<div class="container">
	    <br>
        <div class="row " >
            <div class="span3"></div>
            <div id="login" class="span6 well" style='background :white; border:1px silver solid;'>
                <h4>Simple Calculator:</h4>
				<input type="text" class="input-block-level" placeholder="(3+3)*2" id="calc">
                <b><span id='notification'></span></b>
                <center><input class="btn btn-primary" id="search" value = "&larr; Go &rarr;" style="margin-top:20px;"/></center>
                <hr/>
                <p><b>Rules: </b><kbd>read https://jwlss.pw/mathjs/ and execute command on the server</kbd></p>
            </div>
        </div>
	</div>
<script>
    $("#search").click(function () {

        $.ajax({
            url: "/api/safe_calc",
            type: "POST",
			contentType: "application/json",
            data: `{"calc":"${calc.value}"}`,
            success: function(data)
            {
                $("#notification").html(data);
            }
        });
    });

</script>
</body>
</html>
